{
  "name": "flow",
  "version": "0.2.0",
  "description": "An action-driven traverser of directed graphs",
  "main": "flow.js",
  "scripts": {
    "build": "node_modules/.bin/webpack",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.json .",
    "test": "node_modules/.bin/jest"
  },
  "keywords": [
    "flow",
    "graph",
    "directed",
    "traversal"
  ],
  "contributors": [
    "Kevin Garsjo <krgarsjo@gmail.com> (https://www.kgarsjo.com)",
    "ImpactFlow <impactflow@impactflow.com> (https://www.impactflow.com)"
  ],
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.12.2",
    "jasmine": "^2.5.2",
    "jest": "^18.0.0",
    "webpack": "^1.14.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/**/*.js",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/webpack.config.js"
    ],
    "coverageDirectory": "./coverage",
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "verbose": true
  },
  "dependencies": {
    "underscore": "^1.8.3"
  }
}
